{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "s3:PutObject",
                "s3:GetObjectAcl",
                "s3:GetObject",
                "s3:ListBucket",
                "s3:DeleteObject",
                "s3:PutObjectAcl"
            ],
            "Resource": [
                "arn:aws:s3:::tw-bucket-private/*",
                "arn:aws:s3:::tw-bucket-private",
                "arn:aws:s3:::tw-bucket-public/*",
                "arn:aws:s3:::tw-bucket-public"
            ]
        }
    ]
}
